//Use this file as a base to configure appsettings.

{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=YourPort;Database=YourDataBase;User Id=YourUser;Password=YourPassword;"
  },
  "JwtSetting": {
    "securityKey": "Your strongest password",
    "validAudience": "http://localhost:YourPort",
    "validIssuer": "http://localhost:YourPort"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}